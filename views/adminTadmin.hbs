{{#> plantillaUsuario}}
        <div class="nameTable">
        <h3>Administrar Admin</h3> 
        <img class="iconAdd icons" src="https://img.icons8.com/fluency/30/null/add.png"/>
    </div>
    <div class="table-responsive">
        <table class="table table-sm table-striped table-bordered">
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Nombre</td>
                    <td>Apellidos</td>
                    <td>Email</td>
                    <td>Editar</td>
                    <td>Eliminar</td>
                </tr>
            </thead>
            
            <tbody class="table-group-divider">
                {{#each admin}} 
                    <tr>
                        <td>{{id_usuario}}</td>
                        <td>{{nombre}}</td>
                        <td>{{apellidos}}</td>
                        <td>{{email}}</td>
                        <td>
                            <a onclick="#"><img class="icons" src="https://img.icons8.com/arcade/30/null/edit.png"/></a>
                        </td>
                        <td>
                            <a onclick="deleteAdmin({{id_usuario}}, '{{nombre}}')"><img class="icons" src="https://img.icons8.com/color/29/null/trash--v1.png"/></a>
                        </td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
{{/plantillaUsuario}}